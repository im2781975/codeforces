https://codeforces.com/problemset/problem/1454/A 
#include<stdio.h>
int main()
{
    int t;
    scanf("%d", &t);
    while(t--){
        int n, i, p, j;
        scanf("%d", &n);
        printf("%d", n);
        for(i=1; i<n; i++){
            printf(" %d", i);
        }
        printf("\n");
    }

    return 0;
}
