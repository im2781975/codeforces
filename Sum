https://codeforces.com/problemset/problem/1742/A
#include<stdio.h>
int main()
{
    int t;
    scanf("%d", &t);
    while(t--){
        int a, b, c, flag;
        scanf("%d%d%d", &a, &b, &c);
        if(a+b == c || b+c == a || c+a == b)
            flag=0;
        else
            flag = 1;
        if(flag==0)
            printf("YES\n");
        else if(flag==1)
            printf("No\n");
    }
    return 0;
}
