/* https://codeforces.com/problemset/problem/1787/A?mobile=false */
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,ans=0;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        if(n%2==0)
        {
            cout<<(n/2)<<" "<<1;
        }
        else
        cout<<"-1";
    }
}
