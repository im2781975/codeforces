/* https://codeforces.com/problemset/problem/231/A?mobile=false */
2
0 0 0
1 0 1
1 */
#include<stdio.h>
int main()
{
    int i,j,x,n;
    int solved=0,counter=0;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        for(j=0;j<3;j++)
        {
            scanf("%d",&x);
            counter+=x;
        }
        if(counter>=2)
        solved++;
    }
    printf("%d\n",solved);
}
    
