/* https://codeforces.com/problemset/problem/1790/A?mobile=false */
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,ans=0;
    cin>>t;
    while(t--)
    {
        string PI="314159265358979323846264338327";
        string s;
        cin>>s;
        for(int i=0;i<s.size();i++)
        {
            if(s[i]==PI[i])
            ans++;
            else 
            break;
        }
        cout<<ans;
    }
}
