/* https://codeforces.com/contest/1608/problem/A */
#include<stdio.h>
int main()
{
    int t;
    scanf("%d", &t);
    while(t--){
        int n, i;
        scanf("%d", &n);
        if(n==1){
            printf("1\n");
            continue;
        }

        for(i=2; i<=n+1; i++){
            printf("%d ", i);
        }

        printf("\n");
    }

    return 0;
}
